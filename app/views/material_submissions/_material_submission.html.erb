<tr>
  <td><%= material_submission.labware_type.name %></td>
  <td><%= material_submission.no_of_labwares_required %></td>
  <td><%= material_submission.supply_labwares %></td>
  <td><%= material_submission.updated_at %></td>
  <td>
    <%= if material_submission.active?
      link_to 'View', material_submission_path(material_submission), class: 'btn btn-default'
    else
      link_to 'Continue', material_submission_build_path(id: material_submission.status, material_submission_id: material_submission.id), class: 'btn btn-primary'
    end %>
  </td>
</tr>